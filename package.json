{
  "name": "@gram-data/d3-gram",
  "homepage": "https://github.com/gram-data/d3-gram/",
  "bugs": "https://github.com/gram-data/d3-gram/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/gram-data/d3-gram.git"
  },
  "version": "0.1.13",
  "author": "Andreas Kollegger",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/d3-gram.esm.js",
  "unpkg": "dist/d3-gram.umd.production.min.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --format umd,esm,cjs --tsconfig tsconfig.build.json",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint src --fix",
    "demo": "parcel public/index.html",
    "release": "np --no-tests --no-release-draft --no-yarn",
    "prepare": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint src"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@gram-data/gram-builder": "^0.2.13-alpha.1",
    "@gram-data/gram-stringify": "^0.2.13-alpha.1",
    "@types/d3": "^6.2.0",
    "@types/jest": "^26.0.15",
    "@types/unist": "^2.0.3",
    "@typescript-eslint/parser": "^4.8.2",
    "d3": "^6.2.0",
    "d3-force": "^2.1.1",
    "husky": "^4.3.0",
    "np": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-node-builtins": "^2.1.2",
    "serve": "^11.3.2",
    "shx": "^0.3.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@gram-data/gram-ast": "^0.2.13-alpha.1",
    "@gram-data/gram-ops": "^0.2.13-alpha.1",
    "@gram-data/gram-parse": "^0.2.13-alpha.1",
    "@gram-data/gram-preset-basic": "^0.2.13-alpha.1"
  },
  "gitHead": "a255da2f7cfa6bd16f637d1241d17e10510e60c0"
}
